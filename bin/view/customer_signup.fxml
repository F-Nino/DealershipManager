<?import javafx.scene.text.Font?>

<AnchorPane fx:id="signUpPane" maxHeight="750.0" maxWidth="750.0" minHeight="750.0" minWidth="750.0" prefHeight="750.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.addUser">
   <children>
      <Pane layoutX="-9.0" layoutY="-1.0" prefHeight="750.0" prefWidth="758.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" layoutX="286.0" layoutY="133.0" prefHeight="17.0" prefWidth="212.0" text="Customer Sign-Up">
               <font>
                  <Font name="Arial Black" size="18.0" />
               </font>
            </Label>
            <TextField fx:id="fName" layoutX="312.0" layoutY="188.0" promptText="First  Name" />
            <TextField fx:id="lName" layoutX="312.0" layoutY="246.0" promptText="Last Name" />
            <TextField fx:id="username" layoutX="312.0" layoutY="298.0" promptText="Username" />
            <TextField fx:id="password" layoutX="312.0" layoutY="348.0" promptText="Password" />
            <TextField fx:id="email" layoutX="309.0" layoutY="405.0" prefHeight="27.0" prefWidth="167.0" promptText="Email" />
            <TextField fx:id="age" layoutX="312.0" layoutY="470.0" prefHeight="27.0" prefWidth="49.0" promptText="Age" />
            <TextField fx:id="DOB" layoutX="373.0" layoutY="470.0" prefHeight="27.0" prefWidth="99.0" promptText="Date of Birth" />
            <Button fx:id="button" layoutX="308.0" layoutY="529.0" mnemonicParsing="false" onAction="#onClickAddUser" text="Submit" />
