<AnchorPane fx:id="adminHomePane" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AdminHomeController">
   <children>
      <BorderPane prefHeight="600.0" prefWidth="800.0">
         <top>
            <HBox prefHeight="41.0" prefWidth="750.0" style="-fx-background-color: #ffffff;" BorderPane.alignment="CENTER">
               <children>
                  <Label prefHeight="41.0" prefWidth="79.0" style="-fx-background-color: #BEA519;" text="      Menu">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Label>
                  <Button fx:id="inventory" mnemonicParsing="false" onAction="#changeToInventory" prefHeight="41.0" prefWidth="135.0" style="-fx-background-color: #BEA519;" text="Inventory" />
                  <Button fx:id="purchaseHistory" mnemonicParsing="false" onAction="#changeToPurchaseHistory" prefHeight="41.0" prefWidth="135.0" style="-fx-background-color: #BEA519;" text="Purchase History" />
                  <Button fx:id="manageCustomers" mnemonicParsing="false" prefHeight="41.0" prefWidth="135.0" style="-fx-background-color: #BEA519;" text="Manage Customers" />
