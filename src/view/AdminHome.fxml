                  <Label prefHeight="258.0" prefWidth="617.0" text="                                            Welcome Admin User                                Wel" textOverrun="CLIP">
                     <font>
                        <Font name="System Bold Italic" size="24.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
